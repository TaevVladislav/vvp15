#include <iostream>
using namespace std;
float a; float b;
int Sign(float x) {
	if (x > 0) {
		return 1;
	}
	else if (x == 0) {
		return 0;
	}
	else {
		return -1;
	}
}

int main() {
	cout << "Enter A - ";
	cin >> a;
	cout << "Enter B - ";
	cin >> b;
	cout << "Result = " << Sign(a) + Sign(b) << endl;
}
